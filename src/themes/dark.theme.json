{
  "nav": {
    "container": {
      "position": "fixed",
      "display": "flex",
      "backgroundColor": "#111111",
      "width": "100%",
      "height": "40px",
      "top": "0px",
      "left": "0px",
      "zIndex": "1",
      "alignItems": "center"
    },
    "linksContainer": {
      "display": "flex",
      "height": "40px",
      "width": "100%",
      "paddingLeft": "5px"
    },
    "item": {
      "height": "35px",
      "color": "#F8F4EC"
    },
    "link": {
      "textDecoration": "none",
      "lineHeight": "38px",
      "paddingRight": "5px",
      "paddingLeft": "5px",
      "cursor": "pointer"
    },
    "icon": {
      "height": "30px",
      "width": "30px",
      "marginLeft": "2.5px",
      "minWidth": "30px"
    }
  },
  "home": {
    "container": {
      "display": "flex",
      "height": "calc(100vh - 115px)",
      "paddingTop": "40px",
      "paddingLeft": "15px",
      "paddingRight": "15px",
      "paddingBottom": "15px",
      "backgroundColor": "#555555",
      "justifyContent": "center",
      "alignItems": "center"
    },
    "calculator": {
      "container": {
        "display": "block",
        "height": "500px",
        "width": "400px",
        "border": "2px solid #888888",
        "backgroundColor": "#333333",
        "borderRadius": "5px",
        "paddingTop": "5px",
        "paddingRight": "5px",
        "paddingLeft": "5px",
        "paddingBottom": "5px"
      },
      "input": {
        "width": "calc(100% - 10px)",
        "height": "50px",
        "backgroundColor": "#FFFFFF",
        "textAlign": "right",
        "fontSize": "35px",
        "paddingRight": "10px"
      },
      "body": {
        "display": "flex",
        "height": "calc(100% - 50px)",
        "width": "100%"
      },
      "numpad": {
        "container": {
          "display": "flex",
          "flexWrap": "wrap",
          "height": "100%",
          "width": "75%"
        },
        "button": {
          "height": "25%",
          "width": "33%",
          "color": "#FFFFFF",
          "fontSize": "24px",
          "cursor": "pointer"
        }
      },
      "operators": {
        "container": {
          "display": "flex",
          "flexWrap": "wrap",
          "height": "100%",
          "width": "25%"
        },
        "button": {
          "width": "100%",
          "height": "20%",
          "color": "#FFFFFF",
          "fontSize": "24px",
          "cursor": "pointer"
        }
      }
    }
  },
  "footer": {
    "container": {
      "display": "flex",
      "height": "60px",
      "backgroundColor": "#444444",
      "color": "#F8F4EC",
      "justifyContent": "center",
      "alignItems": "center"
    }
  }
}
