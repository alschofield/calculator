{
  "nav": {
    "container": {
      "position": "fixed",
      "display": "flex",
      "backgroundColor": "#967bb6",
      "width": "calc(100% - 20px)",
      "height": "40px",
      "top": "0px",
      "left": "0px",
      "zIndex": "1",
      "alignItems": "center",
      "justifyContent": "space-between",
      "paddingLeft": "10px",
      "paddingRight": "10px"
    },
    "item": {
      "height": "34px",
      "color": "#111111"
    },
    "icon": {
      "width": "100%",
      "height": "90%",
      "cursor": "pointer"
    }
  },
  "home": {
    "container": {
      "display": "block",
      "height": "calc(100vh - 115px)",
      "paddingTop": "40px",
      "paddingLeft": "15px",
      "paddingRight": "15px",
      "paddingBottom": "15px",
      "backgroundColor": "#F8F4EC"
    },
    "header": {
      "width": "100%",
      "height": "50px",
      "paddingBottom": "15px",
      "display": "flex",
      "justifyContent": "space-between"
    },
    "todolistContainer": {
      "display": "flex",
      "flexWrap": "wrap",
      "height": "100%",
      "justifyContent": "flex-start"
    },
    "addButton": {
      "height": "50px",
      "width": "50px",
      "fontSize": "35px",
      "borderRadius": "5px",
      "backgroundColor": "#111111",
      "color": "#FFFFFF",
      "cursor": "pointer",
      "marginTop": "15px"
    },
    "todolist": {
      "container": {
        "width": "250px",
        "marginTop": "5px",
        "marginRight": "5px",
        "marginBottom": "5px",
        "marginLeft": "5px",
        "paddingTop": "5px",
        "paddingRight": "5px",
        "paddingBottom": "5px",
        "paddingLeft": "5px",
        "height": "fit-content",
        "backgroundColor": "#777777",
        "borderRadius": "5px"
      },
      "header": {
        "textAlign": "center",
        "color": "#FFFFFF"
      },
      "button": {
        "border": "none",
        "cursor": "pointer",
        "paddingTop": "5px",
        "paddingRight": "5px",
        "paddingBottom": "5px",
        "paddingLeft": "5px",
        "backgroundColor": "#FFFFFF",
        "color": "#111111"
      },
      "buttonContainer": {
        "display": "flex",
        "justifyContent": "space-between"
      },
      "list": {},
      "listItem": {
        "cursor": "pointer",
        "width": "fit-content",
        "color": "#FFFFFF"
      },
      "text": {
        "color": "#FFFFFF"
      },
      "innerContainer": {},
      "checkbox": {},
      "input": {
        "width": "90%",
        "backgroundColor": "#FFFFFF"
      }
    }
  },
  "footer": {
    "container": {
      "display": "flex",
      "height": "60px",
      "backgroundColor": "#E7E6DD",
      "color": "#111111",
      "justifyContent": "center",
      "alignItems": "center"
    }
  }
}